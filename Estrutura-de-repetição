#include <stdio.h>

int main (){
	
	int idade, i;
        for(i = 0; i < 3; i++){
    		printf("Digite a idade!\n");
    	    scanf("%d", &idade);
        	    if(idade <= 18)
                    printf("Jovem\n");
                    else printf("Adulto\n"); 
        }	
	    	
        	   
	
	return 0;
}
